<% for( let index = 0; index < messages.length; index++ ) { %>
<div class="alert alert-primary alert-dismissible fade show" role="alert">
	<button
		type="button"
		class="btn-close"
		data-bs-dismiss="alert"
		aria-label="Close"
	></button>
	<strong>Alert </strong> <%= messages[index] %>
</div>

<% } %>
